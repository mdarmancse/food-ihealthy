.noti-popup .viewall-btn {
	padding: 15px;
}

.noti-popup .viewall-btn .btn {
	width: 100%;
	border-radius: 0px;
	font-size: 14px;
	padding: 0.5rem 1rem;
}

.home-banner {
	background: url(../images/doorstep.jpg) no-repeat center center / cover;
	padding: 8rem 0 13rem;
}

.your-doorstep {
	text-align: center;
}

.your-doorstep>h1 {
	font-size: calc(27px + 28 * ((100vw - 300px) / (1920 - 300)));
	font-weight: 700;
	margin: 0 auto 10px;
}

.your-doorstep>p {
	font-size: 25px;
	margin-bottom: 27px;
}

.your-doorstep .search-form {
	width: 100%;
	max-width: 690px;
	margin: auto;
	border-radius: 5px;
	overflow: hidden;
	position: relative;
}

.search-form .form-group {
	margin: 0px;
	display: flex;
}

.search-form .form-group input[type="text"] {
	width: 100%;
	height: 50px;
	border: 0px;
	font-size: 18px;
	padding: 0 15px 0 42px;
